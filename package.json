{
  "displayName": "SESAME OS2 (SESAME3) Plugin",
  "name": "@nzws/homebridge-sesame-os2",
  "version": "1.0.0",
  "description": "Homebridge Plugin for SESAME OS2 (SESAME3)",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nzws/homebridge-sesame-os2.git"
  },
  "bugs": {
    "url": "https://github.com/nzws/homebridge-sesame-os2/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.3.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "LICENSE",
    ".gitignore"
  ],
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "dependencies": {
    "got": "^11.8.2",
    "node-aes-cmac": "^0.1.1"
  },
  "devDependencies": {
    "@nzws/prettier-config": "^1.0.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "homebridge": "^1.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "prettier": "@nzws/prettier-config"
}
